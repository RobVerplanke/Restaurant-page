{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-trailing-spaces": "error",
        "indent": ["error", 2],
        "semi": ["error", "always"],
        "quotes": ["error", "single"]
    }
}
